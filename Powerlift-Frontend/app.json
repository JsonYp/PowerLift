{
  "expo": {
    "name": "PowerLift",
    "slug": "powerlift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/powerlift-icon.png",
    "scheme": "powerlift",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.powerlift.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/powerlift-adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.powerlift.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/powerlift-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/powerlift-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "powerlift-app"
      }
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "nativeVersion"
    }
  }
}
